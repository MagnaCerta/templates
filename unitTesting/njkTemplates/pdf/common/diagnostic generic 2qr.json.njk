{
  "fonts": {
    "Montserrat": {
      "normal": "https://cdn.jsdelivr.net/npm/pstpn-webfont-montserrat@0.0.5/fonts/Regular/Montserrat-Regular.otf",
      "bold": "https://cdn.jsdelivr.net/npm/pstpn-webfont-montserrat@0.0.5/fonts/Bold/Montserrat-Bold.otf"
    }
  },
  "documentDefinition": {
    "pageSize": "A4",
    "pageOrientation": "portrait",
    "background": [
      {
        "image": "data:image/jpeg;base64,",
        "width": 370,
        "height": 80,
        "alignment": "left" 
      },
      {
        "text": "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n",
        "fontSize": 15
      },
      {
        "image": "data:image/jpeg;base64,",
        "width": 560,
        "height": 90,
        "alignment": "left" 
      }
    ],
    "content": [
      {
        "columns": [
          {
            "text": [
              { "text": "\n", "fontSize": 2 },
              {
              "bold": true,
              "text": "VERIFIABLE HEALTH CERTIFICATE",
              "alignment": "left",
              "fontSize": 14
              },
              {
              "bold": true,
              "text": "\nCERTIFICADO DE SALUD VERIFICABLE",
              "alignment": "left",
              "fontSize": 10
              }
            ]
          },
          {
            "image": "{{logo}}",
            "width": 120,
            "height": 40,
            "alignment": "right"
          }
        ],
        "columnGap": 15
      },
      {
        "text": "\n",
        "fontSize": 9
      },
      {
        "table": {
          "widths": ["*"],
          "body": [
            [
              {
                "border": [false, false, false, false],
                "fillColor": "#f3f3f3",
                "text": [
                  { "text": "\n", "fontSize": 1 },
                  { "text": "SUBJECT DETAILS", "bold": true, "fontSize": 9 },
                  { "text": "\nDETALLES DEL SUJETO", "bold": true, "fontSize": 7 },
                  { "text": "\n", "fontSize": 1 }
                ]
              }
            ]
          ]
        }
      },
      {
        "columns": [
          { "text": "", "width": 5 },
          {
            "text": [
              { "text": "\n\nGIVEN NAME |\n", "fontSize": 6, "opacity": 0.8 },
              { "text": "NOMBRE:\n", "fontSize": 6, "opacity": 0.8 },
              { "text": "{{subject.formData.givenName | trim | title}}{% if subject.formData.middleName and (subject.formData.middleName | trim) %} {{subject.formData.middleName | trim | title}}{% endif %}", "fontSize": 9 }
            ]
          },
          {
            "text": [
              { "text": "\n\nLAST NAME |\n", "fontSize": 6, "opacity": 0.8 },
              { "text": "APELLIDO:\n", "fontSize": 6, "opacity": 0.8 },
              { "text": "{{subject.formData.familyName | title}}", "fontSize": 9 }
            ]
          },
          {
            "text": [
              { "text": "\n\nDATE OF BIRTH |\n", "fontSize": 6, "opacity": 0.8 },
              { "text": "FECHA DE NACIMIENTO:\n", "fontSize": 6, "opacity": 0.8 },
              { "text": "{{subject.formData.birthDate | date("yyyy-MM-DD")}}", "fontSize": 9 }
            ]
          },
          {
            "text": [
              { "text": "\n\nGENDER |\n", "fontSize": 6, "opacity": 0.8 },
              { "text": "GÉNERO:\n", "fontSize": 6, "opacity": 0.8 },
              { "text": "{{subject.formData.gender}}", "fontSize": 9 }
            ]
          }
        ]
      },
      {
        "text": "\n\n",
        "fontSize": 9
      },
      {
        "table": {
          "widths": ["*"],
          "body": [
            [
              {
                "border": [false, false, false, false],
                "fillColor": "#f3f3f3",
                "text": [
                  { "text": "\n", "fontSize": 1 },
                  { "text": "PROCEDURE DETAILS", "bold": true, "fontSize": 9 },
                  { "text": "\nDETALLES DEL PROCEDIMIENTO", "bold": true, "fontSize": 7 },
                  { "text": "\n", "fontSize": 1 }
                ]
              }
            ]
          ]
        }
      },
      {
        "text": "\n",
        "fontSize": 3
      },
      {
        "columns": [
          { "text": "", "width": 1 },
          {
            "text": [
              { "text": "PROCEDURE TYPE |\n", "fontSize": 6, "opacity": 0.8 },
              { "text": "TIPO DE PROCEDIMIENTO:\n", "fontSize": 6, "opacity": 0.8 },
              { "text": "{{template.name | safe }}", "fontSize": 9 }
            ]
          },
          {
            "table": {
              "widths": ["*"],
              "body": [
                [
                  {
                    "border": [false, false, false, false],
                    "fillColor": "#f3f3f3",
                     "text": [
                      { "text": "DIAGNOSTIC RESULT |\n", "fontSize": 6, "opacity": 0.8 },
                      { "text": "RESULTADO:\n", "fontSize": 6, "opacity": 0.8 },
                      { "text": "{{subject.formData.observation}}", "fontSize": 9 }
                    ]
                  }
                ]
              ]
            }
          }
        ],
        "columnGap": 4
      },
      {
        "columns": [
          [
            {
              "columns" : [
                { "text": "", "width": 5 },
                {
                  "text": [
                    { "text": "\nSAMPLE COLLECTION DATE |\n", "fontSize": 6, "opacity": 0.8 },
                    { "text": "FECHA DE RECOLECCIÓN DE MUESTRA:\n", "fontSize": 6, "opacity": 0.8 },
                    { "text": "{{subject.formData.sampleDate | date('yyyy-MM-DD')}}", "fontSize": 9 }
                  ]
                }
              ]
            },
            {
              "text": "\n\n\n",
              "fontSize": 9
            },
            {
              "table": {
                "widths": ["*"],
                "body": [
                  [
                    {
                      "border": [false, false, false, false],
                      "fillColor": "#f3f3f3",
                      "text": [
                        { "text": "\n", "fontSize": 1 },
                        { "text": "CERTIFICATE ISSUANCE DETAILS", "bold": true, "fontSize": 9 },
                        { "text": "\nDETALLES DE EMISIÓN DEL CERTIFICADO", "bold": true, "fontSize": 7 },
                        { "text": "\n", "fontSize": 1 }
                      ]
                    }
                  ]
                ]
              }
            },
            {
              "columns" : [
                { "text": "", "width": 5 },
                {
                  "text": [
                    { "text": "\n\nISSUED BY |\n", "fontSize": 6, "opacity": 0.8 },
                    { "text": "EMITIDO POR:\n", "fontSize": 6, "opacity": 0.8 },
                    { "text": "{{digitalPen.legalName}}", "fontSize": 9 }
                  ]
                },
                {
                  "text": [
                    { "text": "\n\nORGANIZATION |\n", "fontSize": 6, "opacity": 0.8 },
                    { "text": "ORGANIZACIÓN:\n", "fontSize": 6, "opacity": 0.8 },
                    { "text": "{{organization.name | safe }}", "fontSize": 9 }
                  ]
                },
                {}
              ]
            },
            {
              "columns" : [
                { "text": "", "width": 5 },
                {
                  "text": [
                    { "text": "\n\nISSUANCE DATE |\n", "fontSize": 6, "opacity": 0.8 },
                    { "text": "FECHA DE EMISIÓN:\n", "fontSize": 6, "opacity": 0.8 },
                    { "text": "{{credential.createdAt | date("yyyy-MM-DD h:mm a") | upper}}", "fontSize": 9 }
                  ]
                },
                {
                  "text": [
                    { "text": "\n\nCOUNTRY OF ISSUANCE |\n", "fontSize": 6, "opacity": 0.8 },
                    { "text": "PAÍS DE EMISIÓN:\n", "fontSize": 6, "opacity": 0.8 },
                    { "text": "{{organization.country | country}}", "fontSize": 9 }
                  ]
                },
                {}
              ]
            }
          ],
          [
            {
              "image": "{{canvas}}",
              "width": 260,
              "height": 140
            },
            {
              "columns" : [
                { "text": "", "width": 5 },
                {
                  "table": {
                    "widths": [240],
                    "body": [
                      [{ "text": "Authorities must download MagnaCerta Verifier app to scan encrypted QR Code and validate authenticity", "fontSize": 7, "alignment": "center"}]
                    ]
                  }
                }
              ]
            }
          ]
        ]
      },
      {
        "text": "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n",
        "fontSize": 8
      },
      {
        "columns": [
          { "text": "", "width": 5 },
          {
            "text": [
              {
                "text": "\n\nWhat can I do with this certificate?\n",
                "fontSize": 9,
                "bold": true
              },
              {
                "text":
                  "\nThis certificate contains data from your diagnostic test or vaccination with elements that make it secure, tamper-proof, verifiable, and does not compromise your privacy. This certificate can be presented to verifying authorities who will scan the QR codes with compatible apps.",
                "fontSize": 7,
                "opacity": 0.8
              },
              {
                "text":
                  "\n\nThese certificates will come in handy now or in the near future in several situations, including traveling to another country, going to school, attending events, accessing corporate buildings, social gatherings, etc. ",
                "fontSize": 7,
                "opacity": 0.8
              }
            ]
          },
          { "text": "", "width": 40 },
          {
            "text": [
              {
                "text": "\n\n¿Qué puedo hacer con este certificado?\n",
                "fontSize": 9,
                "bold": true
              },
              {
                "text":
                  "\nEste certificado contiene datos de su prueba de diagnóstico o vacunación con elementos de seguridad, verificable, no puede ser falsificado y no compromete su privacidad. Se puede presentar a las autoridades de verificación que escanearán los códigos QR con aplicaciones compatibles.",
                "fontSize": 7,
                "opacity": 0.8
              },
              {
                "text":
                  "\n\nEstos certificados serán útiles ahora o en un futuro cercano en varias situaciones, como viajar a otro país, ir a la escuela, asistir a eventos, acceder a edificios corporativos, reuniones sociales, etc.",
                "fontSize": 7,
                "opacity": 0.8
              }
            ]
          }
        ]
      },
      {
        "text": "\n\n",
        "fontSize": 9
      },
      {
        "columns": [
          {
            "image": "data:image/png;base64,",
            "fit": [70, 70],
            "width": "30%"
          },
          [
            {
              "image": "data:image/png;base64,",
              "link":
                "https://apps.apple.com/de/app/magnacerta-wallet/id1539812356?l=en",
              "fit": [55, 55],
              "alignment": "right"
            },
            { "text": "\n", "fontSize": 3 },
            {
              "image": "data:image/png;base64,",
              "link": "https://play.google.com/store/apps/details?id=com.magnacerta.wallet",
              "fit": [55, 55],
              "alignment": "right"
            }
          ],
          {
            "text": [
              { "text": "DOWNLOAD | DESCARGA LAS APPS\n", "fontSize": 7, "bold": true },
              { "text": "\n", "fontSize": 2 },
              {
                "text": "MagnaCerta Wallet ",
                "fontSize": 7,
                "bold": true,
                "color": "#2665ff"
              },
              {
                "text": "to store your certificate | para guardar tu certificado\n",
                "fontSize": 7
              },
              { "text": "\n", "fontSize": 2 },
              {
                "text": "CommonPass ",
                "fontSize": 7,
                "bold": true,
                "color": "#002d74"
              },
              {
                "text": "to travel with your certificate | para viajar con tu certificado\n",
                "fontSize": 7
              },
              { "text": "\n", "fontSize": 2 },
              {
                "text": "MagnaCerta Verifier ",
                "fontSize": 7,
                "bold": true,
                "color": "#ff7a56"
              },
              {
                "text": "to verify the authenticity of this certificate | para verificar la validez ",
                "fontSize": 7
              }
            ],
            "width": "80%"
          }
        ],
        "columnGap": 5
      }
    ],
    "defaultStyle": {
      "font": "Montserrat"
    }
  }
}