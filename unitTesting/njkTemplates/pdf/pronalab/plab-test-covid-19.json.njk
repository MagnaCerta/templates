{
  "fonts": {
    "Montserrat": {
      "normal": "https://cdn.jsdelivr.net/npm/pstpn-webfont-montserrat@0.0.5/fonts/Regular/Montserrat-Regular.otf",
      "bold": "https://cdn.jsdelivr.net/npm/pstpn-webfont-montserrat@0.0.5/fonts/Bold/Montserrat-Bold.otf"
    }
  },
  "documentDefinition": {
    "pageSize": "A4",
    "pageOrientation": "portrait",
    "background": [
      {
        "text": [
          {
            "text":
              "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n____________________________________________________________             ",
            "fontSize": 7.5,
            "alignment": "right",
            "color": "#2665FF"
          },
          {
            "text":
              "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n_____________________________________________________________________________________________________________________________             ",
            "fontSize": 7.5,
            "alignment": "right",
            "color": "#2665FF"
          },
          {
            "text":
              "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n_____________________________________________________________________________________________________________________________             ",
            "fontSize": 7.5,
            "alignment": "right",
            "color": "#2665FF"
          },
          {
            "text":
              "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n_____________________________________________________________________________________________________________________             ",
            "fontSize": 8,
            "alignment": "right",
            "color": "#2665FF"
          },
          {
            "text": "\n",
            "fontSize": 8,
            "alignment": "right",
            "color": "#2665FF"
          }
        ]
      }
    ],
    "content": [
      {
        "columns": [
            [
              {
                  "text": "",
                  "width": "50%"
              },
              {
                "image": "{{logo}}",
                "width": 120,
                "height": 40
              }
            ],
          {
            "text": [
                {
                "bold": true,
                "text": "DIAGNOSTIC LAB REPORT",
                "alignment": "left",
                "fontSize": 14
                },
                {
                "bold": true,
                "text": "\nRESULTADO DE LABORATORIO",
                "alignment": "left",
                "fontSize": 10
                }
            ]
          }
        ],
        "columnGap": 15
      },
      "\n",
      {
        "columns":[
          {
            "image": "{{canvas}}",
            "fit": [260,260],
            "width": "52%"
          },
          [
            {
              "text": "\nSUBJECT DETAILS | DETALLES DEL SUJETO",
              "bold": true,
              "fontSize": 8
            },
            {
              "columns": [
                {
                  "text": [
                    { "text": "\n\nGIVEN NAME | NOMBRE:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.givenName | trim | title}}{% if subject.formData.middleName and (subject.formData.middleName | trim) %} {{subject.formData.middleName | trim | title}}{% endif %}", "fontSize": 8 },
                    {
                      "text": "\n\nLAST NAME | APELLIDO:\n",
                      "fontSize": 5,
                      "opacity": 0.5
                    },
                    { "text": "{{subject.formData.familyName | title}}", "fontSize": 8 },
                    {
                      "text": "\n\nDATE OF BIRTH | FECHA DE NACIMIENTO:\n",
                      "fontSize": 5,
                      "opacity": 0.5
                    },
                    { "text": "{{subject.formData.birthDate | date("yyyy-MM-DD")}}", "fontSize": 8 }
                  ],
                  "width": "50%"
                },
                {
                  "text": [
                    { "text": "\n\nGENDER | GÉNERO:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.gender}}", "fontSize": 8 }
                    {% if subject.formData.passportNumber and (subject.formData.passportNumber | trim) %}
                      ,{ "text": "\n\nPASSPORT NUMBER | NÚMERO DE PASAPORTE::\n", "fontSize": 5, "opacity": 0.5 },
                      { "text": "{{subject.formData.passportNumber}}", "fontSize": 8 }
                    {% endif -%}

                  ],
                  "width": "50%"
                }
              ],
              "columnGap": 10
            },
            {
              {% if subject.formData.passportNumber and (subject.formData.passportNumber | trim) %}
                "text": "\n\n\n\n",
              {% else -%}
                "text": "\n\n\n\n",
              {% endif -%}
              "fontSize": 8
            },
            {
              "text": "\nPROCEDURE DETAILS | DETALLES DEL PROCEDIMIENTO",
              "bold": true,
              "fontSize": 8
            },
            {
              "columns": [
                {
                  "text": [
                    { "text": "\n\nPROCEDURE | ESTUDIO:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.procedure }}", "fontSize": 8 },
                    { "text": "\n\nPROCEDURE TYPE | TÉCNICA:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{template.name | safe }}", "fontSize": 8 },
                    { "text": "\n\nSAMPLE COLLECTION DATE | FECHA DE RECOLECCIÓN DE MUESTRA:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.sampleDate | date("yyyy-MM-DD h:mm a") | upper}}", "fontSize": 8 },
                    { "text": "\n\nDIAGNOSTIC RESULT | RESULTADO:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.observation | title}}", "fontSize": 8 }
                  ],
                  "width": "50%"
                },
                {
                  "text": [                    
                    { "text": "\n\nSPECIMEN COLLECTION METHOD | TIPO DE MUESTRA:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.collectionMethod}}", "fontSize": 8 },
                    { "text": "\n\nSAMPLE QUALITY | CALIDAD DE LA MUESTRA:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.quality}}", "fontSize": 8 }
                    {% if subject.formData.ct and (subject.formData.ct | trim) %}
                      ,{ "text": "\n\nCT (1-38):\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.ct}}", "fontSize": 8 }
                    {% endif -%}
                    {% if subject.formData.analyzer and (subject.formData.analyzer | trim) %}
                      ,{ "text": "\n\nANALYZED BY | ANALIZÓ:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.analyzer | title}}", "fontSize": 8 }
                    {% endif -%}
                    
                  ],
                  "width": "50%"
                }
              ],
              "columnGap": 15
            }
          ]
        ]
      },
      {
        "text": [
          {
            "text":
            "\nPlease be careful not to wrinkle the QR codes when folding so readability is not compromised",
            "fontSize": 5.6
          },
          {
            "text":
            "\nPor favor, procure no doblar los códigos QR para que la legibilidad no se vea comprometida",
            "fontSize": 5.6
          }
        ]
      },
      {
        "text": "\n",
        "bold": true,
        "fontSize": 4
      },
      {
        "text": "\nCERTIFICATE ISSUANCE DETAILS | DETALLES DE EMISIÓN DEL CERTIFICADO",
        "bold": true,
        "fontSize": 8
      },
      {
        "columns": [
          {
            "text": [
              {% if subject.formData.externalReference and (subject.formData.externalReference | trim) %}
                { "text": "\n\nFOLIO NUMBER | NÚMERO DE FOLIO:\n", "fontSize": 5, "opacity": 0.5 },
                { "text": "{{subject.formData.externalReference}}", "fontSize": 8 },
              {% endif -%}
              { "text": "\n\nISSUED BY | EMITIDO POR:\n", "fontSize": 5, "opacity": 0.5 },
              { "text": "{{digitalPen.legalName}}", "fontSize": 8 }
            ]
          },
          {
            "text": [
              { "text": "\n\nISSUANCE DATE | FECHA DE EMISIÓN:\n", "fontSize": 5, "opacity": 0.5 },
              { "text": "{{credential.created | date("yyyy-MM-DD hh:mm a") | upper}}", "fontSize": 8 },
              { "text": "\n\nUNIT | UNIDAD:\n", "fontSize": 5, "opacity": 0.5 },
              { "text": "{{subject.formData.unit}}", "fontSize": 8 }  
            ]
          },
          {
            "text": [
              { "text": "\n\nORGANIZATION | ORGANIZACIÓN:\n", "fontSize": 5, "opacity": 0.5 },
              { "text": "{{organization.name | safe }}", "fontSize": 8 },
              { "text": "\n\nADDRESS | DIRECCIÓN:\n", "fontSize": 5, "opacity": 0.5 },
              { "text": "{{subject.formData.location}}", "fontSize": 8 }
            ],
            "width": "26%"
          },
          {
            "text": [
              {
                "text": "\n\nCOUNTRY OF ISSUANCE | PAÍS DE EMISIÓN:\n",
                "fontSize": 5,
                "opacity": 0.5
              },
              {
                {% if subject.formData.countryName and (subject.formData.countryName | trim) %}
                  "text": "{{subject.formData.countryName | trim }}",
                {% else %}
                  "text": "{{organization.country}}",
                {% endif -%}
                  "fontSize": 8 
              }
            ]
          }
        ],
        "columnGap":5
      },
      {
        "columns": [
          {
            "text": [
              {
                "text": "\n\nAccording to the standardized guidelines for the epidemiological surveillance and the laboratory for 2019- NCOV (SARS-CoV-2) disease, every case with a DETECTED outcome must be notified to the InDRE.",
                "fontSize": 8,
                "opacity": 0.8,
                "alignment": "justify"
              },
              {
                "text":
                  "\nThis test is an auxiliary tool for the diagnostic of the infection caused by the SARS-CoV-2 virus.",
                "fontSize": 8,
                "opacity": 0.8,
                "alignment": "justify"
              },
              {
                "text":
                  "\nAn UNDETECTED result does not rule out being a carrier of the SARS-CoV-2 virus. Follow up is suggested by your physician in order to correlate data with complementary studies.",
                "fontSize": 8,
                "opacity": 0.8,
                "alignment": "justify"
              }
            ],
            "width": "65%"
          },
          [
            {
              "text":
                "\n\n",
              "fontSize": 8
            },
            {
              "image": "data:image/png;base64,",
              "fit": [190, 190],
              "alignment": "left"
            }
          ]
        ],
        "columnGap": 5
      },
      {
        "text": [
          {
            "text": "\n\nWhat can I do with this certificate?\n",
            "fontSize": 10,
            "bold": true
          },
          {
            "text":
              "\nThis certificate contains data from your diagnostic test or vaccination with elements that make it secure, tamper-proof, verifiable, and does not compromise your privacy. This certificate can be presented to verifying authorities who will scan the QR codes with compatible apps.",
            "fontSize": 8,
            "opacity": 0.8,
            "alignment": "justify"
          },
          {
            "text":
              "\n\nThese certificates will come in handy now or in the near future in several situations, including traveling to another country, going to school, attending events, accessing corporate buildings, social gatherings, etc. ",
            "fontSize": 8,
            "opacity": 0.8,
            "alignment": "justify"
          }
        ]
      },
      {
        "text": "\n\n\n\n\n\n\n\n",
        "fontSize": 5.5
      },
      {
        "text": "R01PD 10 V00    ",
        "fontSize": 7.5,
        "opacity": 0.6,
        "alignment": "right"
      },
      {
        "text": "\n\n",
        "fontSize": 5
      },
      {
        "columns": [
          {
            "image": "data:image/png;base64,",
            "fit": [70, 70],
            "width": "25%"
          },
          [
            {
              "image": "data:image/png;base64,",
              "link":
                "https://apps.apple.com/de/app/magnacerta-wallet/id1539812356?l=en",
              "fit": [55, 55],
              "alignment": "right"
            },
            { "text": "\n", "fontSize": 3 },
            {
              "image": "data:image/png;base64,",
              "link": "https://play.google.com/store/apps/details?id=com.magnacerta.wallet",
              "fit": [55, 55],
              "alignment": "right"
            }
          ],
          {
            "text": [
              { "text": "DOWNLOAD | DESCARGA LAS APPS\n", "fontSize": 7, "bold": true },
              { "text": "\n", "fontSize": 2 },
              {
                "text": "MagnaCerta Wallet ",
                "fontSize": 7,
                "bold": true,
                "color": "#2665ff"
              },
              {
                "text": "to store your certificate | para guardar tu certificado\n",
                "fontSize": 7
              },
              { "text": "\n", "fontSize": 2 },
              {
                "text": "CommonPass ",
                "fontSize": 7,
                "bold": true,
                "color": "#002d74"
              },
              {
                "text": "to travel with your certificate | para viajar con tu certificado\n",
                "fontSize": 7
              },
              { "text": "\n", "fontSize": 2 },
              {
                "text": "MagnaCerta Verifier ",
                "fontSize": 7,
                "bold": true,
                "color": "#ff7a56"
              },
              {
                "text": "to verify the authenticity of this certificate | para verificar la validez ",
                "fontSize": 7
              }
            ],
            "width": "80%"
          }
        ],
        "columnGap": 5
      }
    ],
    "defaultStyle": {
      "font": "Montserrat"
    }
  }
}