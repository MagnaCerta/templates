{
  "fonts": {
    "Montserrat": {
      "normal": "https://cdn.jsdelivr.net/npm/pstpn-webfont-montserrat@0.0.5/fonts/Regular/Montserrat-Regular.otf",
      "bold": "https://cdn.jsdelivr.net/npm/pstpn-webfont-montserrat@0.0.5/fonts/Bold/Montserrat-Bold.otf"
    }
  },
  "documentDefinition": {
    "pageSize": "A4",
    "pageOrientation": "portrait",
    "background": [
      {
        "text": [
          {
            "text":
              "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n____________________________________________________________             ",
            "fontSize": 7.5,
            "alignment": "right",
            "color": "#2665FF"
          },
          {
            "text":
              "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n_____________________________________________________________________________________________________________________________             ",
            "fontSize": 7.5,
            "alignment": "right",
            "color": "#2665FF"
          },
          {
            "text":
              "\n\n\n\n\n\n\n\n\n\n_____________________________________________________________________________________________________________________________             ",
            "fontSize": 7.5,
            "alignment": "right",
            "color": "#2665FF"
          },
          {
            "text":
              "\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n_____________________________________________________________________________________________________________________________             ",
            "fontSize": 7.5,
            "alignment": "right",
            "color": "#2665FF"
          },
          {
            "text": "\n",
            "fontSize": 8,
            "alignment": "right",
            "color": "#2665FF"
          }
        ]
      }
    ],
    "content": [
      {
        "columns": [
            [
              {
                  "text": "",
                  "width": "50%"
              },
              {
                "image": "{{logo}}",
                "width": 120,
                "height": 40
              }
            ],
          {
            "text": [
                {
                "bold": true,
                "text": "VERIFIABLE HEALTH CERTIFICATE",
                "alignment": "left",
                "fontSize": 14
                },
                {
                "bold": true,
                "text": "\nCERTIFICADO DE SALUD VERIFICABLE",
                "alignment": "left",
                "fontSize": 10
                }
            ]
          }
        ],
        "columnGap": 15
      },
      "\n",
       {
        "columns":[
          {
            "image": "{{canvas}}",
            "fit": [260,260],
            "width": "53%"
          },
          [
            {
              "text": "\nSUBJECT DETAILS | DETALLES DEL SUJETO",
              "bold": true,
              "fontSize": 8
            },
            {
              "columns": [
                {
                  "text": [
                    { "text": "\n\nGIVEN NAME | NOMBRE:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.givenName | trim | title}}{% if subject.formData.middleName and (subject.formData.middleName | trim) %} {{subject.formData.middleName | trim | title}}{% endif %}", "fontSize": 8 },
                    {
                      "text": "\n\nLAST NAME | APELLIDO:\n",
                      "fontSize": 5,
                      "opacity": 0.5
                    },
                    { "text": "{{subject.formData.familyName | title}}", "fontSize": 8 }
                  ],
                  "width": "50%"
                },
                {
                  "text": [
                    {
                      "text": "\n\nDATE OF BIRTH | FECHA DE NACIMIENTO::\n",
                      "fontSize": 5,
                      "opacity": 0.5
                    },
                    { "text": "{{subject.formData.birthDate | date("yyyy-MM-DD")}}", "fontSize": 8 },
                    { "text": "\n\nGENDER | GÉNERO:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{subject.formData.gender}}", "fontSize": 8 }
                  ],
                  "width": "50%"
                }
              ],
              "columnGap": 10
            },
            {
              "text": "\n\n\n\n\n\n",
              "fontSize": 8
            },
            {
              "text": "\nPROCEDURE DETAILS | DETALLES DEL PROCEDIMIENTO",
              "bold": true,
              "fontSize": 8
            },
            {
              "columns": [
                {
                  "text": [
                    { "text": "\n\nPROCEDURE TYPE | TIPO DE PROCEDIMIENTO:\n", "fontSize": 5, "opacity": 0.5 },
                    { "text": "{{template.name | safe }}", "fontSize": 8 },
                    {
                      "text": "\n\nSAMPLE COLLECTION DATE | FECHA DE RECOLECCIÓN DE MUESTRA:\n",
                      "fontSize": 5,
                      "opacity": 0.5
                    },
                    { "text": "{{subject.formData.sampleDate | date('yyyy-MM-DD h:mm a') | upper}}", "fontSize": 8 }
                  ],
                  "width": "50%"
                },
                {
                  "text": [
                    { "text": "\n\nDIAGNOSTIC RESULT | RESULTADO:\n", "fontSize": 5, "opacity": 0.5 },
                    {% if subject.formData.observation | trim | lower == 'positive' or subject.formData.observation | trim | lower == 'negative' %}
                      { "text": "{{subject.formData.observation | title}}", "fontSize": 8 }
                    {% else -%}
                      { "text": "Unknown", "fontSize": 8 }
                    {% endif -%}
                  ],
                  "width": "50%"
                }
              ],
              "columnGap": 10
            }
          ]
        ]
      },
      {
        "columns":[
          {
            "text":"",
            "width": "2%"
          },
          {
            "text": [
              {
                "text": "AUTHORITIES MUST DOWNLOAD THE ",
                "bold": true,
                "fontSize": 7
              },
              {
                "text": "MAGNACERTA VERIFIER APP",
                "bold": true,
                "fontSize": 7,
                "decoration": "underline"
              },
              {
                "text": " TO SCAN ENCRYPTED QR CODES.",
                "bold": true,
                "fontSize": 7
              }
            ]
          },
          {
            "text":""
          }
        ]
      },
      {
        "text": "\n\n\n",
        "fontSize": 3
      },
      {
        "text": "\nCERTIFICATE ISSUANCE DETAILS | DETALLES DE EMISIÓN DEL CERTIFICADO",
        "bold": true,
        "fontSize": 9
      },
      {
        "columns": [
          {
            "text": [
              { "text": "\n\nISSUED BY | EMITIDO POR:\n", "fontSize": 5, "opacity": 0.5 },
              { "text": "{{digitalPen.legalName}}", "fontSize": 8 }
            ]
          },
          {
            "text": [
              { "text": "\n\nORGANIZATION | ORGANIZACIÓN:\n", "fontSize": 5, "opacity": 0.5 },
              { "text": "{{organization.name | safe }}", "fontSize": 8 }
            ]
          },
          {
            "text": [
              { "text": "\n\nLOCATION | UBICACIÓN:\n", "fontSize": 5, "opacity": 0.5 },
              { "text": "{{subject.formData.location}}", "fontSize": 8 }
            ]
          }
        ],
        "columnGap": 20
      },
      {
        "columns": [
          {
            "text": [
              { "text": "\nISSUANCE DATE | FECHA DE EMISIÓN:\n", "fontSize": 5, "opacity": 0.5 },
              { "text": "{{credential.createdAt | date("yyyy-MM-DD h:mm a") | upper}}", "fontSize": 8 }
            ]
          },
          {
            "text": [
              { "text": "\nCLIA ID:\n", "fontSize": 5, "opacity": 0.5 },
              { "text": "{{subject.formData.cliaId}}", "fontSize": 8 }
            ]
          },
          {
            "text": [
              {
                "text": "\nCOUNTRY OF ISSUANCE | PAÍS DE EMISIÓN:\n",
                "fontSize": 5,
                "opacity": 0.5
              },
              {
                {% if subject.formData.countryName and (subject.formData.countryName | trim) %}
                  "text": "{{subject.formData.countryName | trim }}",
                {% else %}
                  "text": "{{organization.country}}",
                {% endif -%}
                  "fontSize": 8 }
            ]
          }
        ]
        ,
        "columnGap": 20
      },
      {
        "columns": [
          {
            "text": [
              {
                "text": "\n\nWhat can I do with this certificate?\n",
                "fontSize": 10,
                "bold": true
              },
              {
                "text":
                  "\nThis certificate contains data from your diagnostic test or vaccination with elements that make it secure, tamper-proof, verifiable, and does not compromise your privacy. This certificate can be presented to verifying authorities who will scan the QR codes with compatible apps.",
                "fontSize": 8,
                "opacity": 0.8,
                "alignment": "justify"
              },
              {
                "text":
                  "\n\nThese certificates will come in handy now or in the near future in several situations, including traveling to another country, going to school, attending events, accessing corporate buildings, social gatherings, etc. ",
                "fontSize": 8,
                "opacity": 0.8,
                "alignment": "justify"
              }
            ]
          },
          {
            "text": [
              {
                "text": "\n\n¿Qué puedo hacer con este certificado?\n",
                "fontSize": 10,
                "bold": true
              },
              {
                "text":
                  "\nEste certificado contiene datos de su prueba de diagnóstico o vacunación con elementos de seguridad, verificable, no puede ser falsificado y no compromete su privacidad. Se puede presentar a las autoridades de verificación que escanearán los códigos QR con aplicaciones compatibles.",
                "fontSize": 8,
                "opacity": 0.8,
                "alignment": "justify"
              },
              {
                "text":
                  "\n\nEstos certificados serán útiles ahora o en un futuro cercano en varias situaciones, como viajar a otro país, ir a la escuela, asistir a eventos, acceder a edificios corporativos, reuniones sociales, etc.",
                "fontSize": 8,
                "opacity": 0.8,
                "alignment": "justify"
              }
            ]
          }
        ],
        "columnGap": 45
      },
      {
        "text": "\n\n\n\n\n\n\n\n\n\n\n\n",
        "fontSize": 9
      },
      {
        "columns": [
          {
            "image": "data:image/png;base64,",
            "fit": [70, 70],
            "width": "25%"
          },
          [
            {
              "image": "data:image/png;base64,",
              "link":
                "https://apps.apple.com/de/app/magnacerta-wallet/id1539812356?l=en",
              "fit": [55, 55],
              "alignment": "right"
            },
            { "text": "\n", "fontSize": 3 },
            {
              "image": "data:image/png;base64,",
              "link": "https://play.google.com/store/apps/details?id=com.magnacerta.wallet",
              "fit": [55, 55],
              "alignment": "right"
            }
          ],
          {
            "text": [
              { "text": "DOWNLOAD | DESCARGA LAS APPS\n", "fontSize": 7, "bold": true },
              { "text": "\n", "fontSize": 2 },
              {
                "text": "MagnaCerta Wallet ",
                "fontSize": 7,
                "bold": true,
                "color": "#2665ff"
              },
              {
                "text": "to store your certificate | para guardar tu certificado\n",
                "fontSize": 7
              },
              { "text": "\n", "fontSize": 2 },
              {
                "text": "CommonPass ",
                "fontSize": 7,
                "bold": true,
                "color": "#002d74"
              },
              {
                "text": "to travel with your certificate | para viajar con tu certificado\n",
                "fontSize": 7
              },
              { "text": "\n", "fontSize": 2 },
              {
                "text": "MagnaCerta Verifier ",
                "fontSize": 7,
                "bold": true,
                "color": "#ff7a56"
              },
              {
                "text": "to verify the authenticity of this certificate | para verificar la validez ",
                "fontSize": 7
              }
            ],
            "width": "80%"
          }
        ],
        "columnGap": 5
      }
    ],
    "defaultStyle": {
      "font": "Montserrat"
    }
  }
}